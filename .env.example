# Goblin Assistant Environment Configuration
# Copy this file to .env and fill in your actual API keys
# DO NOT commit .env to git - it should be in .gitignore

# Default runtime for development - use 'fastapi' for quick iteration, 'tauri' for production builds
VITE_GOBLIN_RUNTIME=fastapi

# FastAPI backend URL (only used when VITE_GOBLIN_RUNTIME=fastapi)
VITE_FASTAPI_URL=http://127.0.0.1:3001

# Enable mock API for testing when backend is not available
VITE_MOCK_API=true

# Ollama configuration (local models)
OLLAMA_BASE_URL=http://localhost:11434

# llama.cpp server configuration
LLAMACPP_BASE_URL=http://localhost:8080

# LM Studio Local API configuration
# LM Studio provides OpenAI-compatible API on port 1234 by default
LM_STUDIO_BASE_URL=http://localhost:1234

# Cloudflare Vectorize configuration
# Get your account ID from: https://dash.cloudflare.com/
CLOUDFLARE_ACCOUNT_ID=...
# Name of your Vectorize index
VECTORIZE_INDEX_NAME=...

# =============================================================================
# HashiCorp Vault Configuration (Optional - for secure secrets management)
# =============================================================================

# Vault server address and authentication
# VAULT_ADDR=https://vault.example.com:8200
# VAULT_TOKEN=s.xxxxxx

# Vault path for Goblin Assistant secrets
# VAULT_PATH=secret/goblin-assistant

