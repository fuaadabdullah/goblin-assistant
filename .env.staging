# Staging Environment Variables for Goblin Assistant

# Datadog RUM Configuration (Frontend)
VITE_DD_APPLICATION_ID=your_datadog_application_id
VITE_DD_CLIENT_TOKEN=your_datadog_client_token
VITE_DD_ENV=staging
VITE_DD_VERSION=1.0.0

# Legacy Datadog keys (for backend)
DD_API_KEY=your_actual_datadog_api_key
DD_APP_KEY=your_actual_datadog_app_key

POSTGRES_PASSWORD=your_staging_postgres_password
ENV=staging

# Staging backend URL
VITE_FASTAPI_URL=https://staging-api.goblin-assistant.com
VITE_GOBLIN_RUNTIME=fastapi
VITE_MOCK_API=false
